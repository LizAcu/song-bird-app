ria/my-music-app/music-react-frontend/src/reportWebVitals.js",[],"/Users/lisetteacuria/my-music-app/music-react-frontend/src/FileInput/index.js",["62"],"import React, { Component } from 'react'\nimport 'semantic-ui-css/semantic.min.css'\nimport { Form, Button, Label, Icon, Input } from 'semantic-ui-react'\n\nexport default class FileInput extends React.Component {\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            title: '',\n            artist: '',\n            album: '',\n            album_cover: '',\n            likes: 0,\n            file_name: ''\n        }\n\n\n        this.handleSubmit = this.handleSubmit.bind(this);\n        this.handleChange = this.handleChange.bind(this);\n        this.FileInput = React.createRef();\n    }\n\n    handleChange(event) {\n        this.setState({\n            [event.target.name]: event.target.value\n        })\n        // console.log(event.target.value)\n\n    }\n    handleSubmit(event) {\n        //create an object from these attributes from state\n        event.preventDefault();\n        // console.log(this.state)\n        // console.log(this.FileInput)\n        // console.log(this.FileInput.current.inputRef.current.files[0])\n        const newFileName = this.state.file_name.split(\"\\\\\")[2]\n        \n        \n        // fetch(process.env.REACT_APP_API_URL + '/music', {\n            // method: 'POST',\n            // body:\n        // })\n\n        this.props.createMusic({\n            title: this.state.title,\n            artist: this.state.artist,\n            album: this.state.album,\n            album_cover: this.state.album_cover,\n            file_name: newFileName\n\n        })\n\n        // this.setState({\n            // title: '',\n            // artist: '',\n            // album: '',\n            // album_cover: '',\n            // likes: 0,\n            // file_name: ''\n        // })\n\n        // alert(\n            // `Selected file - ${this.FileInput.current.files[0].name}` // undefined\n        // );\n    }\n\n    render() {\n        return (\n            <div>\n            <Form className=\"new-form\" onSubmit={this.handleSubmit}>\n                <Label className=\"title\">Title: \n                    <Input type=\"text\"\n                           name=\"title\"\n                           value={this.state.title}\n                           placeholder=\"Enter a title\"\n                           onChange={this.handleChange}\n                           /> \n                </Label>\n                <Label>Artist:\n                    <Input type=\"text\"\n                           name=\"artist\"\n                           value={this.state.artist}\n                           placeholder=\"Enter an artist\"\n                           onChange={this.handleChange}\n                           /> \n                </Label>\n                <Label>Album:\n                    <Input type=\"text\"\n                           name=\"album\"\n                           value={this.state.album}\n                           placeholder=\"Enter an album -optional-\" \n                           onChange={this.handleChange}\n                           />\n                </Label>\n                <Label>Album Art:\n                    <Input type=\"text\"\n                           name=\"album_cover\"\n                           value={this.state.album_cover}\n                           placeholder=\"Insert album art\"\n                           onChange={this.handleChange}\n                           /> \n                            \n                </Label>\n                <Label>\n                    Upload audio:\n                    <Input type=\"file\"\n                           name=\"file_name\"\n                           ref={this.FileInput}\n                           onChange={this.handleChange}\n                            />\n                </Label>\n                <br />\n                <Button animated=\"vertical\" color=\"blue\" type=\"Submit\">\n                    <Button.Content hidden><Icon name=\"plus circle\" /></Button.Content>\n                    <Button.Content visible>Upload\n                    </Button.Content>\n                    </Button>\n            </Form>\n            <br />\n            </div>\n          \n          \n          \n        );\n    }\n}\n","/Users/lisetteacuria/my-music-app/music-react-frontend/src/MusicContainer/index.js",[],"/Users/lisetteacuria/my-music-app/music-react-frontend/src/Song/index.js",["63","64","65"],"import * as React from 'react';\nimport { Card, Button } from 'semantic-ui-react'\nimport ReactAudioPlayer from 'react-audio-player';\nimport EditMusic from '../EditMusic'\n// import true.mp3 from '../public/mp3Folder/true.mp3';\n// import AudioPlayer from 'react-h5-audio-player'\n// import 'react-h5-audio-player/lib/styles.css'\n\nexport default function Song (props) {\n    console.log(\"I'm in the function song\", props) //logs out as empty array on reload wtf \n\n    const music = props.music.map(music => {\n        console.log(music.file_name)\n        // console.log(music._id)\n        return(\n            <Card.Content key={music._id} textAlign={\"center\"} className=\"cards\">\n                <Card.Header>\n                    <img src={music.album_cover} width=\"250\" height=\"200\" className=\"album-cover-image\"></img>\n                </Card.Header>    \n                <Button color=\"red\"\n                onClick={() => props.deleteMusic(music._id) }>\n                    Delete Song\n                </Button>\n                <Button\n                    color='green'\n                    onClick={ ()=> props.editMusic(music._id) }\n                >\n                    Edit Music\n                </Button>\n               \n                <Card.Description>\n                    <br/>\n                    <h2>{music.title}</h2>\n                    <h3>{music.artist}</h3>\n                </Card.Description>\n                <Card.Header>\n                    <audio\n                        src={music.file_name}\n                        controls\n                        autoPlay\n                        />\n                </Card.Header>\n             </Card.Content>   \n            \n            \n\n        )\n    })\n\n\n    \n       return (\n    <Card.Group centered={true}>\n        {music}\n    </Card.Group>\n)\n       }","/Users/lisetteacuria/my-music-app/music-react-frontend/src/NewUser/index.js",["66"],"import React, { Component } from 'react' \nimport { Form, Button, Label, Icon } from 'semantic-ui-react'\nimport \"../App.css\";\n\n\nexport default class NewUser extends Component {\n    constructor() {\n        super() \n\n        this.state = {\n            email: '',\n            password: '',\n            username: '',\n            action: 'Login'\n        }\n    }\n\nswitchForm = () => {\n    if(this.state.action === 'Login') {\n        this.setState({ action: 'Register' })\n    } else {\n        this.setState({ action: 'Login' })\n    }\n}\n\nhandleChange = (event) => {\n    this.setState({\n        [event.target.name] : event.target.value\n    })\n}\n\nhandleSubmit = (event) => {\n    event.preventDefault()\n    console.log(`You are trying to ${this.state.action.toLowerCase()} with the following credentials`)\n    if(this.state.action === 'Register') {\n        this.props.register({\n            email: this.state.email,\n            password: this.state.password,\n            username: this.state.username\n        })\n    } else {\n        this.props.login(this.state)\n    }\n} \n\n\n    render() {\n        return (\n            <React.Fragment>\n                <h2 className=\"form-label\">{this.state.action} here</h2>\n                <Form onSubmit={this.handleSubmit} className=\"login-form\">\n                    {\n                        this.state.action === 'Register'\n                        &&\n                        <React.Fragment>\n                            <Label>Username:</Label>\n                            <Form.Input\n                              type=\"text\"\n                              name=\"username\"\n                              placeholder=\"Enter a username\"\n                              value={this.state.username}\n                              onChange={this.handleChange}\n                              />  \n                        </React.Fragment>\n                    }\n                    <Label>Email:</Label>\n                    <Form.Input\n                      type=\"email\"\n                      name=\"email\"\n                      placeholder=\"Enter an email\"\n                      value={this.state.email}\n                      onChange={this.handleChange}\n                      />\n                      <Label>Password:</Label>\n                      <Form.Input\n                        type=\"password\"\n                        name=\"password\"\n                        placeholder=\"Enter a password\"\n                        value={this.state.password}\n                        onChange={this.handleChange}\n                        />\n                        <Button type=\"Submit\"\n                                color=\"pink\">\n                            { this.state.action === 'Login' ? 'Log in' : 'Sign up' }\n                        </Button>\n                </Form>\n                {\n                    this.state.action === \"Login\"\n                    ?\n                    <p>\n                        Need an account? Sign up <span className=\"link\" onClick={this.switchForm}>here!</span>\n                    </p>\n                    :\n                    <p>\n                        Already have an account? Login <span className=\"link\" onClick={this.switchForm}>here!</span>\n                    </p>\n                }\n            </React.Fragment>\n        )\n    }\n}","/Users/lisetteacuria/my-music-app/music-react-frontend/src/Header/